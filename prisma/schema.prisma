
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(cuid())
  name String?
  email String? @unique
  username String? @unique
  image String?
  messages Message[]
}

model Message {
  id String @id @default(cuid())
  content String
  createdAt DateTime @default(now())
  user User @relation(fields: [userId], references: [id])
  userId String
}